#include <iostream>
using namespace std;

int max_of_four(int a, int b, int c, int d);

int main() {

  int e, f, g, h;
  cin >> e;
  cin >> f;
  cin >> g;
  cin >> h;

  max_of_four(e, f, g, h);

  return 0;
}

int max_of_four(int a, int b, int c, int d) {

  if ((a > b) && (a > c) && (a > d)) {
    cout << "" << a;
  } else if ((b > a) && (b > c) && (b > d)) {
    cout << "" << b;
  } else if ((c > a) && (c > b) && (c > d)) {
    cout << "" << c;
  } else {
    cout << "" << d;
  }
  return 0;
}
