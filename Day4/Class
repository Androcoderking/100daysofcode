#include <iostream>
using namespace std;

class student
{ 
     public:
     int age;
     string first_name;
     string last_name;
     int standard;

};

int main()
{
    student s;
    cin>>s.age;
    cin>>s.first_name;
    cin>>s.last_name;
    cin>>s.standard;

    cout<<s.age<<endl;
    cout<<s.last_name<<", "<<s.first_name<<endl;
    cout<<s.standard << endl << endl;

    cout<<s.age<<","<<s.first_name<<","<<s.last_name<<","<<s.standard;
   
   return 0;
}

